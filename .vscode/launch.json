  "version": "0.2.0",
  "configurations": [
    {
      "name": "Firebase Functions",
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/dist/apps/coffee-api/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceFolder}/node_modules/**/*.js"
      ]
    }
  ]
}